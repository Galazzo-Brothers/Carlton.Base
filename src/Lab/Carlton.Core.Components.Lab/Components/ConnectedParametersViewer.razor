@namespace Carlton.Core.Components.Lab
@attribute [ViewModelEndpointRefreshPolicy(DataEndpointRefreshPolicy.Never)]
@attribute [ObserveStateEvents<LabStateEvents>(LabStateEvents.MenuItemSelected)]
@attribute [ObserveStateEvents<LabStateEvents>(LabStateEvents.ParametersUpdated)]
@inherits ConnectedComponentBase<ParametersViewerViewModel>


<div class="parameters-viewer">
    <JsonViewerConsole Obj="ViewModel.ComponentParameters.ParameterObj"
                       IsReadOnly="false"
                       OnChangeCallback="obj => base.OnComponentEvent.InvokeAsync(new UpdateParametersCommand(obj))" />
</div>



