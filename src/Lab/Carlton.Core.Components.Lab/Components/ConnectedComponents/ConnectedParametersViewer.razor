@using Carlton.Core.Components.Consoles
@namespace Carlton.Core.Lab.Components
@attribute [ObserveStateEvents("MenuItemSelected")]
@attribute [ObserveStateEvents("ParametersUpdated")]
@inherits BaseConnectedComponent<ParametersViewerViewModel>


<div class="parameters-viewer">
    <JsonViewerConsole Obj="ViewModel.ComponentParameters.ParameterObj"
                       IsReadOnly="false"
                       OnChangeCallback="obj => base.OnComponentEvent.InvokeAsync(new UpdateParametersCommand(obj))" />
</div>



