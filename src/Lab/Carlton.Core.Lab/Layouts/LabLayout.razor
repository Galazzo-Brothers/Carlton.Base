@using Carlton.Core.Layouts
@namespace Carlton.Core.Lab.Layouts
@inherits LayoutComponentBase

<DashboardLayout ShowPanel="true">
    <NavContent>
        <LabFluxComponent TViewModel="NavMenuViewModel" />
    </NavContent>
    <PageTitleContent>
        <LabFluxComponent TViewModel="BreadCrumbsViewModel" />
    </PageTitleContent>
    <HeaderActionContent>
        <LabHeaderActions />
    </HeaderActionContent>
    <Body>
        @Body
    </Body>
    <PanelContent>
       <LabPanelContent />
    </PanelContent>
</DashboardLayout>






