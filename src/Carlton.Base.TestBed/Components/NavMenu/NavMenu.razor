@namespace Carlton.Base.TestBedFramework
@inherits CarltonComponentBase<NavMenuViewModel>


<AccordionSelectGroup TValue="NavMenuItem"
  Groups="ViewModel.MenuItems"
  SelectedItem="ViewModel.SelectedItem"
  OnSelectedItemChanged="async (selectedItem) =>
    await base.OnComponentEvent.InvokeAsync(new NavItemSelected(this, selectedItem.GroupIndexID, selectedItem.ItemIndexID))"/>